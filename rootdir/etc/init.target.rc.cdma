on boot

service kickstart /system/bin/qcks -1 modem_st1 -2 modem_st2 -3 radio_config -4 cdma_record -i /vendor/firmware/
    class core
    user root
    seclabel u:r:kickstart:s0
    oneshot

service setup_fs /system/bin/setup_fs mmcblk0p38 mmcblk0p39
    class core
    user root
    group root
    oneshot
