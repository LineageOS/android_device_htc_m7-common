on boot

service fm_dl /system/bin/setprop hw.fm.init 1
    class late_start
    user root
    group system fm_radio
    disabled
    oneshot

service kickstart /system/bin/qcks -1 modem_st1 -2 modem_st2 -3 radio_config -4 cdma_record -i /firmware/mdm/image/ -u
    class core
    user root
    oneshot

service setup_fs /system/bin/setup_fs mmcblk0p36
    class core
    user root
    group root
    oneshot
