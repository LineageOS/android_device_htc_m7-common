# Android fstab file.
#<src>	<mnt_point>	<type>	<mnt_flags>	<fs_mgr_flags>
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

/dev/block/mmcblk0p35	/boot		emmc	defaults						defaults
/dev/block/mmcblk0p36	/recovery	emmc	defaults						defaults
/dev/block/mmcblk0p37	/system		ext4	rw,noatime,barrier=1					wait
/dev/block/mmcblk0p38	/cache		ext4	nosuid,nodev,noatime,barrier=1				wait,check
/dev/block/mmcblk0p39	/data		ext4	noatime,nosuid,nodev,noauto_da_alloc,barrier=1		wait,check,encryptable=footer
/dev/block/mmcblk0p33	/firmware/mdm	vfat	ro,fmask=0000,dmask=0000,shortname=lower,context=u:object_r:radio_efs_file:s0		wait
/dev/block/mmcblk0p16	/firmware/q6	vfat	ro,fmask=0000,dmask=0000,shortname=lower,context=u:object_r:radio_efs_file:s0		wait

# USB storage
/devices/platform/msm_hsusb_host/usb	/storage/usbdisk	auto	defaults			voldmanaged=usbdisk:auto
